datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = "native"
}

model User {
  id                  Int       @id @default(autoincrement())
  email               String    @unique
  hashedPassword      String
  salt                String
  resetToken          String?
  resetTokenExpiresAt DateTime?
}

model Asset {
  id          Int    @id @default(autoincrement())
  assetName   String
  address     String
  axisTracker String
  hemisphere  String
  panelTilt   Int
  panelType   String
  totalDc     Float
  totalAc     Float
  lat         Float
  lon         Float
}
